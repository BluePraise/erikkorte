.theme-gallery .gallery-item a:before{
    position:absolute;
    content:'';
    height:0%;
    width:100%;
    background-color: #3c3950ba;
    top:0;
    bottom:auto;
    left:0;
    right:0;
    transition:1s;
}
.theme-gallery .gallery-item a:hover:before{
    height:100%;
}
.theme-gallery .gallery-item a:after{
    position:absolute;
    content:'';
    inset:0;
    margin:auto;
    color:var(--theme-color5);
    display:flex;
    align-items:center;
    justify-content:center;
    opacity:0;
	font-size:0px;
    width:60px;
    height:60px;
    background-color:var(--theme-color1);
    border-radius:50%;
    background-size:30px;
    background-position:center;
    background-repeat:no-repeat;
     background-image:url('/24/ek/wp-content/uploads/2024/12/camera-icon.png');
	transform:scale(0);
}
.theme-gallery .gallery-item a:hover:after{
    opacity:1;
     transition:.5s;
	transform:scale(1);
}
.theme-gallery .title-caption-col{
    position:absolute;
    bottom:35px;
    right:0;
    left:0;
    text-align:center;
    color:white;
    opacity:0;
    transition:2s;
}
.theme-gallery .title-caption-col h3{
    color:white;
    font-size:22px;
    margin-bottom:0;
}
.theme-gallery .gallery-item a:hover .title-caption-col{
    opacity:1;
}
.theme-gallery .title-caption-col p{
    margin-bottom:0;
}
.lb-container img{
    height:700px !important;
    width:700px !important;
