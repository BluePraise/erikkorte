/* This theme's styles are loaded from assets/css/style.css */

/* Helpers */
.d-block {
    display: block;
}
.d-flex {
    display: flex;
}
.unlist {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

.row {
    --bs-gutter-x: 42px;
}
body {
    background-color: #e8ecef;
    color: #5f727f;
    margin:0;
    padding: 0;
}
.site-wrapper {
    max-width: 1260px;
    margin: 0 auto;
    padding: 0;
    box-shadow: 0 0 5px rgba(85, 57, 57, 0.11);
    background-color: #fff;
}

/* Top Area */
.top-area-block {
    background-color: #edeef3;
    padding: 12.5px 0px;
    border-bottom: 1px solid #ddd;
    color: #5f727f;
}

.contacts {
    align-items: center;
    justify-content: center;
}
.contacts li {
    display: inline-flex;
    margin: 0 20px;
    align-items: center;
}
.contacts li p {
    color: #8896A0;
    font-family: var(--Source-Sans-Pro);
    font-size: 14px;
}
.contacts li a {
    font-family: var(--Source-Sans-Pro);
    color: #8896A0;
    font-size: 14px;
    text-decoration: none;
}
.contacts li i {
    margin-right: 5px;
    font-size: 20px;
}
/* End Top Area */

/* Header */
.main-nav {
    display: flex;
    justify-content: space-between;
    background-color: #fff;
    padding: 20px;
}
.custom-logo-link {
    display: inline-block;
    margin-right: 20px;
}
header p {
    margin-bottom: 0;
}

.site-header .main-menu {
    list-style: none;
    margin: 0;
    padding: 0;
    display: flex;
    align-items: center;
}
.site-header .main-menu li {
    font-family: "Montserrat", sans-serif;
    font-weight: 700;
    font-size: 14px;
    text-decoration: none;
    margin-right: 20px;
    position: relative;
}

.site-header .main-menu li a {
    text-decoration: none;
    text-transform: uppercase;
    font-size: 14px;
    color: #3c3950;
    padding: 0px 8px;
    transition: var(--transition);
    font-family: "Montserrat", sans-serif;
    padding-top: 3px;
    padding-bottom: 3px;
}
.site-header .main-menu li.current-menu-parent>a {
    color: var(--theme-color1);
    background-color: #e7edf1;
}
.site-header .main-menu li a:hover {
    color: var(--theme-color1);
}
/* End Header */

.page-title-block {
    background-size: cover;
    background-position: center;
    color: var(--bs-white);
    text-align: center;
    padding: 100px 0; /* Adjust top and bottom padding */
}

.page-title-title h1 {
    font-size: 3rem;
    font-weight: bold;
    margin-bottom: 20px;
    text-transform: uppercase;
}
#crumbs>* {
    margin: 0 10px;
    padding-bottom: 10px;
    border-bottom-width: 2px;
    border-bottom-style: solid;
}
.breadcrumbs-container {
     margin-top: 0;
    text-align: center;
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    font-weight: 300;
    letter-spacing: 1px;
    font-size: 16px;
    font-weight: 700;
    text-decoration: none;
    color: var(--bs-white);
    padding: 6px 10px 10px;
    & a {
        color: inherit;
        text-decoration: none;
        transition: var(--transition);
        border-bottom-color: transparent;
        &:hover {
            border-bottom-color: var(--bs-white);
        }
    }
}

.breadcrumbs {
    display: inline-block;
    margin-bottom: 10px;
}

.breadcrumbs .divider {
    margin: 0 10px;
}

.breadcrumbs a {
    color: var(--bs-white);
    text-decoration: none;
    transition: color 0.3s ease;
}

.breadcrumbs a:hover {
    color: #f39c12; /* Hover effect for breadcrumb links */
}

.breadcrumbs .current {
    color: #f39c12; /* Current page title color */
}
.hero-banner {
    display: flex;
    align-items: center;
    min-height: 350px;
    padding: 0 !important;
    background-position: center;
    position: relative;
}
.banner-heading {
    margin-bottom: 0;
    font-family: var(--Montserrat);
}

/*--almelo--*/
.icon_box_info {
    display: flex;
    align-items: center;
    justify-content: center;
}
.icon_box_info.service-image img{
    height:40px;
    width:40px;
    object-fit:contain;
}

.quickfinder-item-box:hover .icon_box_info.service-image img{
  filter: brightness(0) saturate(100%) invert(20%) sepia(42%) saturate(4505%) hue-rotate(346deg) brightness(92%) contrast(96%);
}

/*--almelo-close--*/
/*--Candel-Table-css-start--*/
.candle-modal-dialog .candle-authors>div {
    text-align: center;
    padding: 14px;
    display: flex;
    justify-content: center;
    flex-direction: column;
    color: var(--bs-black);
    gap: 5px;
    margin: 0px 10px 0px;
}
.candle-modal-dialog .candle-authors>div p.candle-author-name {
    margin: 0;
    font-weight: 600;
    font-size: 18px;
    text-transform: capitalize;
}
.candle-modal-dialog .candle-authors>div time:before {
    content: "\f073";
    font-family: 'FontAwesome';
    padding-right: 7px;
    font-size: 12px;
}
.candle-modal-dialog button.candle-modal-close.close {
    background: var(--theme-color3) !important;
    width: 50px;
    height: 50px;
    border-radius: 10px;
    color: var(--bs-black);
    font-size: 30px !important;
}
/*--Candel-table-css-close--*/
/*--testimonials--*/
.review_slider h3 {
    font-family: 'Montserrat';
    font-weight: 700;
    font-size: 14px;
    color: var(--bs-white);
}
/*--testimonials-close--*/
/*--Border-Biutton--*/
.border-btn{
    border:2px solid #b2201e;
    padding:5px 15px;
    text-transform:uppercase;
    font-weight:600;
}

/*--Border-Button-Close--*/
/*--Footer--*/
footer.site-footer {
    background-color: var(--body-color);
    color: var(--bs-white);
    padding: 110px 20px;
}

footer .menu li a {
    line-height: 25px;
    letter-spacing: 0.05em;
    font-weight: normal;
    font-size: 14px;
    color: var(--theme-color3);
    -o-transition: all 0.2s linear;
    -webkit-transition: all 0.2s linear;
    transition: all 0.2s linear;
    text-decoration: none;
}
footer .menu li a:hover {
    text-decoration: underline;
}
footer .menu li:before {
    border-radius: 50%;
    content: '';
    width: 5px;
    margin-top: 10px;
    height: 5px;
    float: left;
    margin-left: -14px;
    background-color: var(--theme-color1);
}

footer .widget-title:after {
    content: "";
    clip-path: polygon(0 0, 0% 100%, 100% 100%);
    width: 9px;
    height: 8px;
    position: relative;
    display: inline-block;
    background: var(--theme-color1);
    margin-left: 5px;
}
footer .menu {
    list-style: disc outside none;
    margin-bottom: 1em;
    margin-left: .875em;
    padding-left: .875em;
    margin-top: -17px;
}
.footer-nav .menu {
    display: inline-block;
    margin: 0 -21px;
    text-align: center;
}

.footer-nav .menu li {
    display: inline-block;
    padding: 0 21px;
    position: relative;
    line-height: 1;
}

.footer-nav .menu li a {
    font-size: 14.86px;
    display: block;
    font-weight: normal !important;
    color: var(--theme-color3) !important;
    text-decoration: none;
    transition: var(--transition);
}
.footer-nav .menu li a:hover {
    text-decoration: underline;
}
.footer-nav .menu li + li:before {
    content: "";
    width: 0;
    height: 100%;
    position: absolute;
    left: -1px;
    top: 0;
    font-size: 0;
    border-left: 1px solid #232234;
    border-right: 1px solid #333146;
}
/*--Footer-close--*/
/* --inner-pages-- */
.inner-page-outer h2,
.gallery-content-section h2 {
    font-size: 17px;
    color: var(--theme-color4);
    text-transform: uppercase;
    letter-spacing: 0.05em;
    font-weight: 700;
    font-family: var(--Montserrat);
}
.btn {
    font-size: 14px;
    background-color: var(--theme-color1);
    color: var(--bs-white);
    font-weight: 700;
    padding: 8.5px 17px;
    font-family: var(--Montserrat);
    border-radius: 3px;
}
.btn:hover {
    background-color: var(--theme-color5);
    color: var(--bs-white);
}
/* --inner-pages-close-- */
/* --contact-- */
.contact-info-row h2 {
    color:#808080;
    font-size:19px;
    font-weight:400;
    text-transform:uppercase;
    font-family: var(--Montserrat);
    text-align:center;
}
.contact-info-row p, .contact-info-row a{
    color:#5f727f;
    font-size:14px;
    font-weight:500;
    text-transform:capitalize;
    font-family: var(--Montserrat);
    text-align:center;
    text-decoration:none;
}
.contact-info-row .icon img{
    display: block;
    margin:auto;
    width:40px;
    padding-bottom:30px;
}
.contact-form .flex-col p{
    display: flex;
    gap:10px;
}
.contact-form label {
    display: block;
    flex: 1 0 0%;
    width: 100%;
    max-width: 100%;
}
.contact-form .wpcf7-form .wpcf7-form-control:not(input[type="radio" i], input[type="checkbox" i], .wpcf7-submit) {
    width: 100%;
    padding: 12px;
    background-color: #f4f6f7;
    border: 1px solid #dfe5e8;
    border-radius: 0;
    margin-bottom: 0px;
    max-height: 150px;
}
.contact-form .wpcf7-response-output {
    float: left;
    width: 100%;
    margin: 0 !important;
}
.contact-form .wpcf7-form input[type='submit'] {
    height: 50px;
    line-height: 30px;
    padding: 10px 40px;
    font-size: 20px;
    background-color: var(--theme-color1);
    width:100%;
    margin: 15px 15px 15px 0;
    font-family: var(--Montserrat);
   font-weight:700;
   color:var(--bs-white);
    text-transform:uppercase;
    border-radius:0px;
}
.contact-form .wpcf7-form input[type='submit']:hover{
    background-color: #3c3950;
}

/* --contact-close-- */
/* --blog-- */
.blog-posts .post {
    margin-bottom: 40px;
}
.post-img img {
    max-width: 100%;
    height: auto;
    aspect-ratio: 1.2;
    object-fit: cover;
}
.post-content h3 {
    font-size: 19px;
    text-transform: uppercase;
    font-weight: normal;
    font-family: var(--Montserrat);
    letter-spacing: 0.05em;
    margin-bottom: 7px;
    border-bottom: 1px solid #ccc;
    padding-bottom: 3px;
}
.post-content h3 a {
    color: inherit;
    font-family: inherit;
}
.post-content p:empty {
    display: none;
}
.post-content .btn {
    padding: 2px 9px;
    font-size: 80%;
}
.post-pagination {
    display: flex;
    margin-top: 70px;
    padding-top: 30px;
    border-top: 1px solid #ccc;
}
.post-pagination>* {
    display: inline-block;
    font-family: var(--Montserrat);
    padding: 1.5px 10px;
    font-weight: 700;
    color: #cfd7dd;
    border: 1px solid #cfd7dd;
    background-color: #ffffff;
    margin-right: 10px;
    min-width: 30px;
    text-align: center;
    font-size: 14px;
    transition: var(--transition);
}
.post-pagination>*:hover, .post-pagination>.current {
    border-color:var(--theme-color1);
    background-color: var(--theme-color1);
    color: var(--bs-white);
}
.single-post .post-content,
.single-post .post-meta{
    max-width: 800px;
    margin: 60px auto;
}
.comment-form input:not([type="radio" i], [type="checkbox" i], [type=submit]), .comment-form textarea {
    width: 100%;
    padding: 9px 20px;
    border: 1px solid #dfe5e8;
    color: #3c3950;
    background-color: #f4f6f7;
    outline: none;
    max-height: 82px;
    height: auto;
}

.comment-form .form-submit [type=submit] {
    font-size: 14px;
    background-color: var(--theme-color1);
    color: var(--bs-white);
    font-weight: 700;
/*     padding: 8.5px 17px; */
    font-family: var(--Montserrat);
    border-radius: 3px;
    border: 0;
}
.comment-form .form-submit [type=submit]:hover {
    background-color: var(--theme-color5);
    color: var(--bs-white);
}
.comment-form {
    display: flex;
    flex-wrap: wrap;
    column-gap: 22px;
}

.comment-form>* {
    flex-shrink: 0;
    width: 100%;
    max-width: 100%;
}

.comment-form-author, .comment-form-email, .comment-form-url {
    flex: 0 0 auto;
    width: calc(33.33333333% - 15px);
}
